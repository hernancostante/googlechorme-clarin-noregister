{
  "name": "Clarin no-register",
  "description": "My first Chrome App :) ",
  "version": "0.3",
  "manifest_version": 2,
  "content_scripts" : [
    {
     "matches" : ["*://*.clarin.com/*"],
     "js" : ["clarin.js"]
    },
    {
     "matches" : ["*://*.lanacion.com.ar/*"],
     "js" : ["lanacion.js"]
    }
   ],
  "icons": { "16": "icon.png"}
}
